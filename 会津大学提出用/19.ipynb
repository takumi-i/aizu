    "    h, w = map(int,input().split())\n",
    "    if h == w == 0:\n",
    "        break\n",
    "    for i in range(h):\n",
    "        for j in range(w):\n",
    "            if (i+j) % 2 == 0:\n",
    "                print(\"#\",end=\"\")\n",
    "            else:\n",
    "                print(\".\",end=\"\")\n",
    "        print()"
