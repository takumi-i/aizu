    "import math\n",
    "while True:\n",
    "    #SS = 0\n",
    "    n = int(input())\n",
    "    if n == 0:\n",
    "        break\n",
    "    s = list(map(int, input().split()))\n",
    "    m = sum(s) / n\n",
    "    #print(m)\n",
    "    #SS = 0\n",
    "    SS = sum(((i - m) ** 2) for i in s)\n",
    "    #print(SS)\n",
    "    A = math.sqrt(SS / n)\n",
    "    print(A)"
   ]
