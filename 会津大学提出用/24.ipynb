    "\n",
    "n, m = map(int, input().split())\n",
    "a = [[0 for i in range(m)] for j in range(n)]\n",
    "b = [0 for i in range(m)]\n",
    "for i in range(n):\n",
    "    a[i] = list(map(int, input().split()))\n",
    "for j in range(m):\n",
    "    b[j] = int(input())\n",
    "np.dot(a, b)"
