    "n = int(input())\n",
    "TS = 0\n",
    "HS = 0\n",
    "for i in range(n):\n",
    "    taro, hana = map(str, input().split())\n",
    "    if taro > hana:\n",
    "        TS = TS + 3\n",
    "    elif hana > taro:\n",
    "        HS = HS + 3\n",
    "    else:\n",
    "        TS = TS + 1\n",
    "        HS = HS + 1\n",
    "print(str(TS) + \" \" + str(HS))"
   ]
